    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cobe": "^0.6.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "echo-editor": "^0.5.0",
    "express": "^5.1.0",
    "fast-average-color": "^9.5.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "helmet": "^8.1.0",
    "hextaui": "^1.2.8",
