{
  "name": "ixstats",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:no-check": "next build --no-lint",
    "build:fast": "SKIP_ENV_VALIDATION=true next build --no-lint",
    "check": "next lint && tsc --noEmit",
    "dev": "./start-development.sh",
    "start": "next start -p ${PORT:-3000}",
    "start:dev": "./start-development.sh",
    "start:prod": "./start-production.sh",
    "deploy:prod": "./scripts/deploy-production.sh",
    "preview": "next build && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:studio": "prisma studio",
    "db:init": "tsx scripts/init-db.ts",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:init",
    "db:seed": "tsx scripts/seed-db.ts",
    "db:backup": "tsx scripts/backup-db.ts",
    "db:restore": "tsx scripts/restore-db.ts",
    "dev:db": "npm run db:setup && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next dist build",
    "clean:all": "npm run clean && rm -rf node_modules && rm -rf prisma/dev.db",
    "fresh": "npm run clean:all && npm install && npm run db:setup",
    "websocket:server": "tsx src/lib/websocket-server.ts",
    "websocket:dev": "tsx watch src/lib/websocket-server.ts",
    "auth:check": "node scripts/check-auth-config.js",
    "auth:check:dev": "NODE_ENV=development node scripts/check-auth-config.js",
    "auth:check:prod": "NODE_ENV=production node scripts/check-auth-config.js",
    "auth:validate:prod": "node scripts/validate-clerk-production.js",
    "clerk:setup": "node scripts/setup-clerk-dev.js",
    "clerk:config": "echo 'See scripts/clerk-dashboard-config.md for Clerk dashboard configuration'",
    "validate:servers": "./scripts/validate-server-config.sh"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.1",
