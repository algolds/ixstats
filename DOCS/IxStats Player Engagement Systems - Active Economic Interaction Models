## üîÑ **Internal-External Factor Integration**
*"Every domestic issue has international dimensions, every international issue has domestic consequences"*

### **Multi-Dimensional Crisis Cascades**
```typescript
interface CascadingCrisisSystem {
  // Internal issues with external factor sensitivity
  internalIssues: {
    unemployment_crisis: {
      internalFactors: ["automation", "skills_mismatch", "business_confidence"],
      externalTriggers: ["trade_disruption", "global_recession", "supply_chain_shifts"],
      
      cascadeEffects: {
        economic: "consumer_spending_drop ‚Üí business_revenue_loss ‚Üí more_layoffs",
        political: "anti_incumbent_sentiment ‚Üí policy_uncertainty ‚Üí investment_hesitancy",
        social: "mental_health_decline ‚Üí family_stress ‚Üí community_breakdown",
        cultural: "work_identity_crisis ‚Üí value_system_questioning ‚Üí generational_conflict"
      },
      
      externalSpillover: {
        trading_partners: "reduced_import_demand + migration_pressure",
        competitors: "competitive_advantage_shifts + market_share_gains",
        allies: "support_requests + burden_sharing_negotiations"
      }
    },
    
    political_polarization: {
      internalFactors: ["inequality", "cultural_change", "institutional_trust"],
      externalTriggers: ["foreign_influence", "refugee_flows", "economic_competition"],
      
      cascadeEffects: {
        governance: "policy_gridlock ‚Üí infrastructure_decay ‚Üí competitiveness_loss",
        diplomacy: "inconsistent_foreign_policy ‚Üí ally_confusion ‚Üí isolation_risk",
        economy: "regulatory_uncertainty ‚Üí business_hesitancy ‚Üí growth_slowdown",
        society: "violence_risk ‚Üí brain_drain ‚Üí social_capital_erosion"
      },
      
      externalSpillover: {
        neighbors: "instability_contagion + border_security_concerns",
        investors: "political_risk_premium + capital_flight",
        international_system: "alliance_reliability_questions + power_vacuum_creation"
      }
    },
    
    environmental_degradation: {
      internalFactors: ["industrial_pollution", "resource_extraction", "urban_sprawl"],
      externalTriggers: ["climate_change", "transboundary_pollution", "global_consumption"],
      
      cascadeEffects: {
        health: "disease_burden_increase ‚Üí healthcare_costs ‚Üí productivity_loss",
        economy: "ecosystem_service_loss ‚Üí agricultural_decline ‚Üí tourism_damage",
        migration: "environmental_refugees ‚Üí urban_pressure ‚Üí social_tension",
        security: "resource_conflicts ‚Üí border_disputes ‚Üí military_spending"
      },
      
      externalSpillover: {
        region: "pollution_export + migration_flows + resource_competition",
        global: "climate_contribution + biodiversity_loss + supply_chain_disruption"
      }
    }
  };
}

// Example: How a domestic housing crisis becomes international
const housingCrisisSystem: CrisisSystem = {
  internalTriggers: {
    primary: "speculative_investment + zoning_restrictions + income_stagnation",
    accelerators: "population_growth + credit_expansion + construction_costs"
  },
  
  internalCascades: {
    economic: [
      "housing_unaffordability ‚Üí household_debt_increase ‚Üí consumption_reduction",
      "construction_bubble ‚Üí bank_exposure ‚Üí financial_system_risk",
      "housing_wealth_inequality ‚Üí social_tension ‚Üí political_instability"
    ],
    
    social: [
      "young_adult_housing_exclusion ‚Üí delayed_family_formation ‚Üí demographic_crisis",
      "geographic_mobility_reduction ‚Üí labor_market_rigidity ‚Üí productivity_loss",
      "homelessness_increase ‚Üí public_health_crisis ‚Üí urban_decay"
    ],
    
    political: [
      "generational_conflict ‚Üí electoral_realignment ‚Üí policy_uncertainty",
      "local_government_revenue_dependence ‚Üí perverse_incentives ‚Üí corruption_risk"
    ]
  },
  
  externalConnections: {
    causes: {
      "foreign_investment_inflows": "international_capital_seeking_safe_assets",
      "global_monetary_policy": "low_interest_rates_encouraging_speculation", 
      "immigration_pressure": "population_growth_exceeding_housing_supply"
    },
    
    effects: {
      "brain_drain_trigger": "talented_youth_emigrating_for_affordable_housing",
      "international_reputation": "social_inequality_affecting_soft_power",
      "economic_competitiveness": "high_living_costs_deterring_business_investment"
    },
    
    policy_interactions: {
      "immigration_policy": "housing_costs_affecting_migration_attractiveness",
      "trade_policy": "construction_materials_tariffs_affecting_housing_costs",
      "foreign_investment_policy": "capital_controls_vs_housing_affordability_tradeoffs"
    }
  },
  
  playerEngagementRequirements: {
    immediate_decisions: [
      "foreign_investment_restrictions_vs_capital_flight_risk",
      "public_housing_investment_vs_fiscal_deficit_concerns",
      "zoning_reform_vs_existing_homeowner_opposition"
    ],
    
    international_negotiations: [
      "bilateral_construction_worker_agreements",
      "building_materials_trade_negotiations", 
      "urban_planning_expertise_sharing_agreements"
    ],
    
    complex_tradeoffs: {
      analysis_required: "housing_affordability + economic_growth + social_cohesion + international_competitiveness",
      stakeholder_management: "homeowners vs renters vs developers vs international_investors",
      temporal_considerations: "short_term_relief vs long_term_sustainability",
      spillover_management: "domestic_solutions_affecting_international_relations"
    }
  }
};
```

